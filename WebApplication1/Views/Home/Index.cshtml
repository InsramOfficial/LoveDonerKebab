@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";

    @if (TempData["Message"] != null)
    {
        <script>
            Swal.fire({
                toast: true,
                position: 'top-end', // Top-right corner
                icon: 'success',    // Icon type (success, error, etc.)
                title: '<strong style="font-size: 16px; color: #333;">@TempData["Message"]</strong>',
                showConfirmButton: false, // No confirm button
                timer: 3000,       // 3 seconds duration
                timerProgressBar: true // Show progress bar
            });
        </script>
    }

    @if (TempData["SuccessMessage"] != null)
    {
        <script>
            Swal.fire({
                toast: true,
                position: 'top-end', // Top-right corner
                icon: 'success',    // Icon type (success, error, etc.)
                title: '<strong style="font-size: 16px; color: #333;">@TempData["SuccessMessage"]</strong>',
                showConfirmButton: false, // No confirm button
                timer: 3000,       // 3 seconds duration
                timerProgressBar: true // Show progress bar
            });
        </script>
    }

    @if (TempData["ErrorMessage"] != null)
    {
        <script>
            Swal.fire({
                toast: true,
                position: 'top-end', // Top-right corner
                icon: 'success',    // Icon type (success, error, etc.)
                title: '<strong style="font-size: 16px; color: #333;">@TempData["ErrorMessage"]</strong>',
                showConfirmButton: false, // No confirm button
                timer: 7000,       // 3 seconds duration
                timerProgressBar: true // Show progress bar
            });
        </script>
    }

}
